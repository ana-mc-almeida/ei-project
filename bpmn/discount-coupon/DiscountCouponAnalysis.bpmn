    <bpmn:serviceTask id="Activity_17dni39" name="Obtain Client From Coupon">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="url">http://${KONG_ADDRESS}:8000/DiscountCoupon/${couponId}</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="accept">application/json</camunda:entry>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="idLoyaltyCard">
              <camunda:script scriptFormat="JavaScript">S(connector.getVariable("response")).jsonPath("idLoyaltyCard").numberValue();</camunda:script>
            </camunda:outputParameter>
            <camunda:outputParameter name="expirationDate">
              <camunda:script scriptFormat="JavaScript">S(connector.getVariable("response")).jsonPath("expirationDate").stringValue();</camunda:script>
            </camunda:outputParameter>
            <camunda:outputParameter name="idsShops_ignore">
              <camunda:script scriptFormat="JavaScript">var response = S(connector.getVariable("response"));

var arr = response.prop("idsShops");

var arrValues = [];

for (var i = 0; i &lt; arr.elements().size(); i++) {
  arrValues.push(arr.elements().get(i).value()); 
}

connector.setVariable("idsShops", arrValues.join(", "));</camunda:script>
            </camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_06usou0</bpmn:incoming>
      <bpmn:outgoing>Flow_0lri7ro</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_1kc8xs8" name="Obtain All Purchases From Last Month">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="url">http://${KONG_ADDRESS}:8000/Purchase</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="accept">application/json</camunda:entry>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="ClientPurchases_ignored">
              <camunda:script scriptFormat="JavaScript">var responseString = connector.getVariable("response");
var response = S(responseString).elements();

var targetId = connector.getVariable("idLoyaltyCard");

var filtered = [];

var expirationDate = new Date(connector.getVariable("expirationDate"));
var oneMonthBeforeExpirationDate = new Date(expirationDate);
oneMonthBeforeExpirationDate.setMonth(expirationDate.getMonth() - 1);

var shops = connector.getVariable("idsShops").split(", ").map(s =&gt; parseInt(s));

connector.setVariable("shops_debbug", S(JSON.stringify(shops)));

for (var i = 0; i &lt; response.size(); i++) {
  var item = response.get(i);
  var timestampStr = item.prop("timestamp").value();

  var purchaseDate = new Date(timestampStr);

  if (
item.prop("loyaltyCard_id").value() == targetId 
&amp;&amp; purchaseDate &gt;= oneMonthBeforeExpirationDate 
&amp;&amp; purchaseDate &lt;= expirationDate
&amp;&amp; shops.includes(item.prop("shop_id").value())
) {
filtered.push({
      id: item.prop("id").value(),
      timestamp: item.prop("timestamp").value(),
      price: item.prop("price").value(),
      product: item.prop("product").value(),
      supplier: item.prop("supplier").value(),
      shop_id: item.prop("shop_id").value(),
      loyaltyCard_id: targetId,
      discountCoupon_id: item.prop("discountCoupon_id").value()
    });
 }
}



connector.setVariable("ClientPurchases", S(JSON.stringify(filtered)));
</camunda:script>
            </camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0lri7ro</bpmn:incoming>
      <bpmn:outgoing>Flow_1qxs27z</bpmn:outgoing>
    </bpmn:serviceTask>
